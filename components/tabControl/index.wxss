/* components/tabControl/index.wxss */
.TestTabbar {
  display: flex;
  height: 60rpx;
  padding: 16rpx 0;
  border-bottom: 2rpx solid #e2a140;
}
.TestTabbar-item {
  flex: 1;
  text-align: center;
  line-height: 60rpx;
  font-weight: 550;
  color: #000000;
}
.TestTabbarActive {
  color: #e2a140;
}
.TestTabbarActive .TestTabbarText {
  border-bottom: #e2a140 solid 6rpx;
  padding: 18rpx 1rpx;
}
